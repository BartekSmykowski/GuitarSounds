<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<GridPane hgap="10" prefHeight="350.0" prefWidth="800.0" vgap="10" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <HBox prefHeight="350.0" prefWidth="800.0">
         <children>
            <Pane fx:id="menuButtonsPane" prefHeight="350.0" prefWidth="61.0" />
            <VBox prefHeight="350.0" prefWidth="739.0">
               <children>
                  <Pane fx:id="neckPane" nodeOrientation="LEFT_TO_RIGHT" prefHeight="177.0" prefWidth="198.0" />
                  <HBox prefHeight="173.0" prefWidth="739.0" spacing="20.0">
                     <children>
                        <Pane fx:id="chordsButtonsPane" prefHeight="173.0" prefWidth="211.0" />
                        <Pane fx:id="tabChoosePane" prefHeight="173.0" prefWidth="284.0">
                           <children>
                              <Label fx:id="tabNameLabel" layoutX="14.0" layoutY="14.0" text="Choose tabulature" />
                              <Button fx:id="tabChooseButton" layoutX="14.0" layoutY="47.0" mnemonicParsing="false" onMouseClicked="#openTabChooser" text="Choose tabulature" />
                              <Button fx:id="playTabButton" layoutX="14.0" layoutY="117.0" mnemonicParsing="false" onMouseClicked="#playLoadedTabulature" prefHeight="25.0" prefWidth="52.0" text="Play" />
                              <Label layoutX="14.0" layoutY="87.0" text="Set frequency:" />
                              <TextField fx:id="frequencyTextField" layoutX="100.0" layoutY="83.0" prefHeight="25.0" prefWidth="58.0" text="5" />
                              <ProgressBar fx:id="melodyProgressBar" layoutX="11.0" layoutY="150.0" prefWidth="200.0" progress="0.0" />
                              <Button fx:id="stopButton" layoutX="74.0" layoutY="117.0" mnemonicParsing="false" onMouseClicked="#stopPlaying" prefHeight="25.0" prefWidth="52.0" text="Stop" />
                              <Button fx:id="pauseButton" layoutX="132.0" layoutY="117.0" mnemonicParsing="false" onMouseClicked="#pauseClicked" text="Pause" />
                              <Button fx:id="resumeButton" layoutX="185.0" layoutY="117.0" mnemonicParsing="false" onMouseClicked="#resumeClicked" text="Resume" />
                           </children>
                        </Pane>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</GridPane>
